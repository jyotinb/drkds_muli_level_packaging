<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for package numbering -->
        <record id="seq_stock_package" model="ir.sequence">
            <field name="name">Stock Package</field>
            <field name="code">stock.package</field>
            <field name="prefix"></field>  <!-- No prefix, just sequential numbers -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>